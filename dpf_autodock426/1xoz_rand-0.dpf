outlev 1                             # diagnostic output level
intelec                              # calculate internal electrostatics
seed pid time                        # seeds for random generator
ligand_types A C OA HD N
fld data/1xoz/protein.maps.fld
map data/1xoz/protein.A.map
map data/1xoz/protein.C.map
map data/1xoz/protein.OA.map
map data/1xoz/protein.HD.map
map data/1xoz/protein.N.map
elecmap data/1xoz/protein.e.map
desolvmap data/1xoz/protein.d.map
move  data/1xoz/rand-0.pdbqt

about    6.260   -3.797    9.950  
tran0 random                         # initial coordinates/A or random
quat0 random                         # initial quaternion
dihe0 random                         # initial dihedrals (relative) or random
rmstol 2.0                           # cluster_tolerance/A
extnrg 1000.0                        # external grid energy
e0max 0.0 10000                      # max initial energy; max number of retries
ga_pop_size 150
ga_num_evals 2048000
ga_num_generations 9999999
ga_elitism 1                         # number of top individuals to survive to next generation
ga_mutation_rate 0.02                # rate of gene mutation
ga_crossover_rate 0.8                # rate of crossover
ga_window_size 10                    # 
ga_cauchy_alpha 0.0                  # Alpha parameter of Cauchy distribution
ga_cauchy_beta 1.0                   # Beta parameter Cauchy distribution
ls_search_freq 1.000000
set_ga                               # set the above parameters for GA or LGA
sw_max_its 300
sw_max_succ 4                   # consecutive successes before changing rho
sw_max_fail 4                   # consecutive failures before changing rho
sw_rho 1.0                           # size of local search space to sample
sw_lb_rho 0.01                       # lower bound on rho
set_sw1                              # set the above Solis & Wets parameters
ga_run 100
rmsref data/1xoz/flex-xray.pdbqt
analysis                             # perform a ranked cluster analysis
